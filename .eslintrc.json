{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "env": {
    "es6": true,
    "mocha": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "chrome": true,
    "console": true
  },
  "rules": {
    "no-console": [
      "error",
      {
        "allow": [
          "log"
        ]
      }
    ],
    "no-underscore-dangle": 0
  }
}
